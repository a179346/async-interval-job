{
    "parser": "@typescript-eslint/parser",
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended", "prettier"],
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "no-console": "error",
        "prettier/prettier": [
          "error",
          {
            "endOfLine": "auto"
          }
        ]
    }
}
